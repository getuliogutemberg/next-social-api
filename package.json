{
  "name": "next-social-api",
  "version": "0.1.0",
  "private": true,
 
  "scripts": {
    "build": "concurrently \"npm run server\" \"npm run startClient\"",
    "start": "concurrently \"npm run server\" \"npm run startClient\"",
    "startClient": "next start",
    "buildClient": "next build",
    "startServer": "node server/server.js -p 5000",
    "dev": "concurrently \"npm run devServer\" \"npm run devServer\"",
    "devServer": "nodemon server/server.js ",
    "devClient": "next dev",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.4",
    "axios": "^1.6.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "firebase": "^10.6.0",
    "next": "14.0.1",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "react-spinners": "^0.13.8"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0"
  }
}
